{"ast":null,"code":"import { ref } from \"vue\";\nimport { db } from \"../firebase/config\";\nimport { addDoc, collection } from \"firebase/firestore\";\nexport default {\n  setup() {\n    //patient info const\n    const fullname = ref(\"\");\n    const email = ref(\"\");\n    const dob = ref(\"\");\n    const nric = ref(\"\");\n    const note = ref(\"\");\n    const contact = ref(\"\");\n    const address = ref(\"\");\n\n    const handleSubmit = async () => {\n      const colRef = collection(db, \"user\");\n      await addDoc(colRef, {\n        fullname: fullname.value,\n        email: email.value,\n        dob: dob.value,\n        nric: nric.value,\n        note: note.value,\n        contact: contact.value,\n        address: address.value\n      });\n      alert(\"Patient added successfully\");\n      fullname.value = \"\";\n      email.value = \"\";\n      dob.value = \"\";\n      nric.value = \"\";\n      note.value = \"\";\n      contact.value = \"\";\n      address.value = \"\";\n    };\n\n    return {\n      handleSubmit,\n      fullname,\n      email,\n      dob,\n      nric,\n      note,\n      contact,\n      address\n    };\n  }\n\n};","map":{"version":3,"sources":["D:\\Laragon\\www\\Osem-Salmon-PRS\\src\\components\\CreateUserForm.vue"],"names":[],"mappings":"AAuDA,SAAS,GAAT,QAAoB,KAApB;AACA,SAAS,EAAT,QAAmB,oBAAnB;AACA,SAAS,MAAT,EAAiB,UAAjB,QAAmC,oBAAnC;AAGA,eAAe;EACb,KAAK,GAAG;IACN;IACA,MAAM,QAAO,GAAI,GAAG,CAAC,EAAD,CAApB;IACA,MAAM,KAAI,GAAI,GAAG,CAAC,EAAD,CAAjB;IACA,MAAM,GAAE,GAAI,GAAG,CAAC,EAAD,CAAf;IACA,MAAM,IAAG,GAAI,GAAG,CAAC,EAAD,CAAhB;IACA,MAAM,IAAG,GAAI,GAAG,CAAC,EAAD,CAAhB;IACA,MAAM,OAAM,GAAI,GAAG,CAAC,EAAD,CAAnB;IACA,MAAM,OAAM,GAAI,GAAG,CAAC,EAAD,CAAnB;;IAEA,MAAM,YAAW,GAAI,YAAY;MAC/B,MAAM,MAAK,GAAI,UAAU,CAAC,EAAD,EAAK,MAAL,CAAzB;MAEA,MAAM,MAAM,CAAC,MAAD,EAAS;QACnB,QAAQ,EAAE,QAAQ,CAAC,KADA;QAEnB,KAAK,EAAE,KAAK,CAAC,KAFM;QAGnB,GAAG,EAAE,GAAG,CAAC,KAHU;QAInB,IAAI,EAAE,IAAI,CAAC,KAJQ;QAKnB,IAAI,EAAE,IAAI,CAAC,KALQ;QAMnB,OAAO,EAAE,OAAO,CAAC,KANE;QA