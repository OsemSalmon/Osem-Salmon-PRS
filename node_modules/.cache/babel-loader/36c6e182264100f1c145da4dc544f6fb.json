er()\n    const route = useRoute()\n\n    const handleClick = () => {\n      signOut(auth)\n      router.push('/login')\n    }\n\n    watchEffect(() => {\n      if(!userAuth.value) {\n        if(route.name !== 'Login' && route.name !== 'Signup') {\n          router.push('/login')\n        }\n      }\n    })\n\n    return { handleClick, userAuth, }\n  }\n}\n</script>\n\n<style>\n</style>"],"sourceRoot":""},"metadata":{},"sourceType":"module"}